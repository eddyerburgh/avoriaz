{
  "name": "avoriaz",
  "version": "1.1.1",
  "description": "a Vue.js testing utility library",
  "main": "build",
  "scripts": {
    "build": "babel src --out-dir dist",
    "docs:build": "npm run docs:prepare && rimraf _book && gitbook build",
    "docs:prepare": "gitbook install",
    "docs:publish": "npm run docs:build && cd _book && cd docs && git init && git commit --allow-empty -m 'Update docs' && git fetch git@github.com:eddyerburgh/avoriaz.git gh-pages && git checkout -b gh-pages && git add . && git commit -am 'Update docs' && git push git@github.com:eddyerburgh/avoriaz.git gh-pages --force",
    "docs:watch": "cd docs && gitbook serve",
    "lint": "eslint  --ext .js,.vue src test --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint:fix && npm run test:karma",
    "test:karma": "cross-env BABEL_ENV=test karma start test/unit/setup/karma.conf.js --single-run",
    "test:unit": "mocha-webpack --webpack-config test/unit/setup/webpack.config.test.js test/unit/specs --recursive --require test/unit/setup/.mocha-webpack-setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eddyerburgh/avoriaz.git"
  },
  "keywords": [
    "test vue",
    "vue.js",
    "vue.js 2",
    "vue",
    "vue test",
    "vue.js test",
    "vue.js 2 test",
    "vue 2 test",
    "vue testing utils",
    "vue.js testing utils",
    "vue 2 testing utils",
    "vue.js 2 testing utils",
    "vue testing utilities",
    "vue.js testing utilities",
    "vue 2 testing utilities",
    "vue.js 2 testing utilities"
  ],
  "author": "Edd Yerburgh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eddyerburgh/avoriaz/issues"
  },
  "homepage": "https://github.com/eddyerburgh/avoriaz#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-html": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "gitbook-cli": "^2.3.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.9.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.1",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "vue": "^2.1.8",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^1.14.0"
  },
  "peerDependencies": {
    "vue": "2.x"
  }
}
